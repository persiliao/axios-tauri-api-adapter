{
  "name": "axios-tauri-api-adapter",
  "version": "0.2.1",
  "description": "Makes it easy to use Axios in Tauri App",
  "keywords": [
    "axios",
    "tauri",
    "api",
    "http",
    "adapter"
  ],
  "homepage": "https://github.com/persiliao/axios-tauri-api-adapter#readme",
  "bugs": {
    "url": "https://github.com/persiliao/axios-tauri-api-adapter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/persiliao/axios-tauri-api-adapter.git"
  },
  "license": "MIT",
  "author": "Persi.Liao",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build-prepare": "rimraf lib",
    "format": "prettier --write \"src/**/*.ts\"",
    "preinstall": "npx only-allow pnpm",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "run-s format build-prepare build",
    "test": "jest --config jestconfig.json"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.2",
    "axios": "^1.3.3",
    "build-url-ts": "^6.1.3",
    "http-status-codes": "^2.2.0",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.5",
    "@types/url-parse": "^1.4.8",
    "conventional-changelog-cli": "^2.2.2",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "only-allow": "^1.1.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4",
    "uuid-generator-ts": "^1.1.0"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}
